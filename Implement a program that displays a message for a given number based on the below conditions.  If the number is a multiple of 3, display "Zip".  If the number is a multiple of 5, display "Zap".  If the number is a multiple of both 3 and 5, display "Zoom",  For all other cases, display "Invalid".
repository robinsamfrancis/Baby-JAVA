class Tester {
	public static void main(String[] args) {
	    int num = 11;
	    if (num % 3 == 0 && num % 5 ==0){
	        System.out.println("Zoom");
	    } 
	    else if (num%3 == 0){
	        System.out.println("Zip");
	    } 
	    else if (num%5 == 0){
	        System.out.println("Zap");
	    } 
	   
	    else {
	        System.out.println("Invalid");
	    }
	    
		// Implement your code here 
	}
}

