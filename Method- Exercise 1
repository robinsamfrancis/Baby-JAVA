class Calculator {
    public static double findAverage(int number1, int number2, int number3){
        double sum = number1+number2+number3;
        double average = sum/3;
        double roundOff = Math.round(average*100.0)/100.0;
        return roundOff;
    }

	// Implement your code here
}

class Tester {

	public static void main(String args[]) {
		Calculator calculator = new Calculator();
		System.out.println(calculator.findAverage(12,8,15));
		// Invoke the method findAverage of the Calculator class and display the average
	}
}
