package basics2;

//CALCULATION WITH INSTANCE VARIABLES OF AN OBJECT

public class Demo2 {
public static void main(String[]args) {
		
		
		aircraft aircraft1 = new aircraft(); // creates object of class tester
		aircraft aircraft2 = new aircraft(); // creates another object of class
		
		double aircraft1Endurance;
		double aircraft2Endurance;
		
		
		aircraft1.fuelCapacity= 6;
		aircraft1.passsengers=4;
		aircraft1.fuelBurnRate= 56.5;
		aircraft1.speed= 120;
		
		
		
		
		aircraft2.fuelCapacity= 10;
		aircraft2.passsengers=40;
		aircraft2.fuelBurnRate= 569.5;
		aircraft2.speed= 1200;
		
		aircraft1Endurance= aircraft1.fuelCapacity/aircraft1.fuelBurnRate;
		System.out.println("Endurance of aircraft "aircraft1Endurance);
		
		aircraft2Endurance= aircraft2.fuelCapacity/aircraft2.fuelBurnRate;
		System.out.println("Endurance of aircraft 1 is" + aircraft2Endurance);
		
		
		
		
	}

}


class aircraft{
	int passsengers;      
	int speed;               
	double fuelCapacity;    
	double fuelBurnRate;  
	
	
	
}


